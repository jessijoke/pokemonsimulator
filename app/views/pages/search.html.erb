<div class="page_title">
    <h1 class="h1_title">
       Search for Users
    </h1>
</div>

<div class="page_content">

    <div class="divider">    </div>

        <%= form_with url: search_results_path do |f| %>
            <%= f.label "Enter a username or email:" %>
            <%= f.text_field :entry %>
            
            <%= f.submit "Search" %>
        <% end %>

 </div>

